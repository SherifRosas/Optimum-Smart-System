{
  "version": 2,
  "builds": [
    {
      "src": "optimum-frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://sherifrosas.pythonanywhere.com/api/$1"
    }
  ]
}


